<h2>検索結果一覧</h2>

<table class="table table-hover table-inverse">
  <!--検索対象モデルがHospitalの時 -->
  <% if @range == "Hospital" %>
    <tbody>
      <% @hospitals.each do |hospital| %>
        <tr>
          <td><%= hospital.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <!--検索対象モデルがHospitalではない時(= 検索対象モデルがGenreの時) -->
    <tbody>
      <% @genres.each do |genre| %>
        <tr>
          <td><%= genre.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>