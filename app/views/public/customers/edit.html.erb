<h2>プロフィール編集</h2>
<%= form_with model: @customer do |f| %>
  <label for="inputName">ニックネーム</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <label for="inputAddress">住んでいる都道府県</label>
  <%= f.text_field :address, id:"inputAddress", placeholder:"住んでいる都道府県"%>
  <label for="inputImage">プロフィール画像</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.submit "変更を保存" %>
<% end %>